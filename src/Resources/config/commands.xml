<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
  <services>
    <service id="Nucleos\UserBundle\Command\ActivateUserCommand">
      <argument type="service" id="nucleos_user.util.user_manipulator"/>
      <tag name="console.command" command="nucleos:user:activate"/>
    </service>
    <service id="Nucleos\UserBundle\Command\ChangePasswordCommand">
      <argument type="service" id="nucleos_user.util.user_manipulator"/>
      <tag name="console.command" command="nucleos:user:change-password"/>
    </service>
    <service id="Nucleos\UserBundle\Command\CreateUserCommand">
      <argument type="service" id="nucleos_user.util.user_manipulator"/>
      <tag name="console.command" command="nucleos:user:create"/>
    </service>
    <service id="Nucleos\UserBundle\Command\DeactivateUserCommand">
      <argument type="service" id="nucleos_user.util.user_manipulator"/>
      <tag name="console.command" command="nucleos:user:deactivate"/>
    </service>
    <service id="Nucleos\UserBundle\Command\DemoteUserCommand">
      <argument type="service" id="nucleos_user.util.user_manipulator"/>
      <tag name="console.command" command="nucleos:user:demote"/>
    </service>
    <service id="Nucleos\UserBundle\Command\PromoteUserCommand">
      <argument type="service" id="nucleos_user.util.user_manipulator"/>
      <tag name="console.command" command="nucleos:user:promote"/>
    </service>
  </services>
</container>
